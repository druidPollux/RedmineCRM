<h2><%=l(:label_cms_menu_new)%></h2>

<%= labelled_form_for :cms_menu, @cms_menu, :url => { :action => 'create', :id => @cms_menu},
  :html => { :id => 'cms_menu-form', :method => :post } do |f| %>
<%= render :partial => 'form', :locals => { :f => f } %>
<%= submit_tag l(:button_create) %>
<% end %>