<h2><%= l(:label_products_new) %></h2>

<%= labelled_form_for :product, @product, :url => {:action => 'create', :project_id => @project}, :html => { :multipart => true, :id => 'product_form'} do |f|  %>
  <%= render :partial => 'form', :locals => {:f => f}  %>
  <%= submit_tag l(:button_save) -%>
<% end -%>

